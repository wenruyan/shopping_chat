}
.menu .menu-item::after {
  content: '';
  width: 670rpx;
  height: 2rpx;
  background-color: #F4F4F4;
  position: absolute;
  bottom: 0;
  left: 0;
}
.menu .menu-item:last-child::after {
  height: 0;
}
.menu .menu-item .right-icon {
