}
.order .order-wrap {
  margin: 0 auto;
width:670rpx;
height:180rpx;
background:rgba(255,255,255,1);
box-shadow:0px 12rpx 26rpx 0px rgba(55,57,70,0.08);
border-radius:20rpx;
line-height:180rpx;
text-align: center;
}
.order .order-wrap .item {
  position: relative;
  width: 25%;
  height:180rpx;
}
.order .order-wrap .item .icon {
  width: 50rpx;
  height: 50rpx;
}
.order .order-wrap .item .tip {
  width: 27rpx;
  height: 27rpx;
  line-height: 27rpx;
  font-size: 18rpx;
  background-color:#FC5110;

  color:#ffffff;
  border-radius:50%;
  position: absolute;
  top:35rpx;
  left: 50%;
}

.order .order-wrap .item.shipped .tip {
  /* color: #A578FF; */
  border-color: #A578FF;
}
.order .order-wrap .item.complete .tip {
  color: #FECD10;
  border-color: #FECD10;
}
.order .order-wrap .item .text {
  font-size: 22rpx;
  color: #666666;
 margin-top:-143rpx;
 font-weight: Medium;

