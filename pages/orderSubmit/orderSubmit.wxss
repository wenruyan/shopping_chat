.soccer-account {
  width: 670rpx;
  background-color: #fff;
  height: calc(100% - 688rpx);
  position: relative;
  padding: 20rpx 40rpx 0;
  
}

.soccer-account::after {
  position: absolute;
  content: '';
  width: 100%;
  height: 20rpx;
  background-color: #F9F9F9;
  top: 0;
  left: 0;
}

.soccer-account .title {
  height: 105rpx;
  line-height: 105rpx;
  border-bottom: 1rpx solid #E6E6E6;
  font-size: 36rpx;
  color: #333333;
  font-weight: bold;
}

.soccer-account .content {
  padding-top: 30rpx;
  font-size: 28rpx;
}

.soccer-account .content view:first-child {
  font-weight: bold;
}